{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "0.2",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/mjaquiery/SeePassword",
  "icons": {
    "48": "icons/seePassword-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "web_accessible_resources": [
    "icons/eye.svg"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/seepass.js"],
      "css": ["content_scripts/iconstyle.css"]
    }
  ]
}
